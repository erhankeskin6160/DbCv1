@using DbCv1.Models.Entity;
@model  List<TblEğitimlerim>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}




<br />
<h2 style="color:blue">Eğitim Bilgisi Düzenleme Paneli</h2>

<table class="table table-bordered table-stripped">
    <tr>
        <th>Id</th>
        <th>Baslık</th>
        <th>Alt Baslık</th>
        <th>Alt Baslık2</th>
        <th>GNO</th>
        <th>Tarih</th>
        <th>Sil</th>
        <th>Düzenle</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Baslık</td>
            <td>@item.AltBaslık</td>
            <td>@item.AltBaslık</td>
            <td>@item.GNO</td>
            <td>@item.Tarih</td>
            <td><a href="/Eğitim/EğitimSil/@item.Id" class="btn btn-danger ">Sil</a></td>
            <td><a href="/Eğitim/EğitimDüzenle/@item.Id" class="btn btn-warning">Düzenle</a></td>
        </tr>
    }
    <tr></tr>
</table>
<a href="/Eğitim/EğitimEkle" class="btn btn-primary">Yeni Eğitim Bilgisi Ekle</a>
