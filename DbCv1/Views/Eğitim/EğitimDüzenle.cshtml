@model DbCv1.Models.Entity.TblEğitimlerim
@{
    ViewBag.Title = "EğitimDüzenle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h4>Eğitim Bilgisi Düzenleme Sayfası</h4>
<br />
<form method="post">

    @Html.Label("Baslık Açıklaması")
    @Html.TextBoxFor(x => x.Baslık, new { @class = "form-control" }) 
    @Html.ValidationMessageFor(x => x.Baslık, "", new { @style = "color:red" })
    <br />

    @Html.Label("Alt Baslık1")
    @Html.TextBoxFor(x => x.AltBaslık, new { @class = "form-control" })

    @Html.Label("Alt Baslık2")
    @Html.TextBoxFor(x => x.AltBaslık2, new { @class = "form-control" })

    @Html.Label("Tarih")
    @Html.TextBoxFor(x => x.Tarih, new { @class = "form-control" })

    @Html.Label("Genel Not Ortalaması")
    @Html.TextBoxFor(x => x.GNO, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.GNO, "", new { @style = "color:red" })




    <br />


    <button class="btn btn-info">Kaydet</button>
</form>