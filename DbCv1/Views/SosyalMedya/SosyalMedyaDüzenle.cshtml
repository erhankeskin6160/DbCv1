@using DbCv1.Models.Entity
@model  TblSosyalMedya


@{
    ViewBag.Title = "SosyalMedyaDüzenle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Sosyal Medya Düzenleme Sayfası</h2>

 
 
<br />
<form method="post">

    @Html.Label("Sosyal Medya İsmi")
    @Html.TextBoxFor(x => x.Ad, new { @class = "form-control" })

    @Html.Label("Sosyal Medya Linki")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })

    @Html.Label("Sosyal Medya İconu ")
    @Html.TextBoxFor(x => x.Icon, new { @class = "form-control" })


    <br />


    <button class="btn btn-info">Güncelle</button>
</form>