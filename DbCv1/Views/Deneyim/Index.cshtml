@using DbCv1.Models.Entity;
@model  List<TblDeneyimler>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2  style="color:blue">Deneyim Düzenleme Paneli</h2>

<table class="table table-bordered table-stripped">
    <tr>
        <th>Id</th>
        <th>Baslık</th>
        <th>Alt Baslık</th>
        <th>Tarih</th>
        <th>Sil</th>
        <th>Düzenle</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Baslık</td>
            <td>@item.Alt_Baslık</td> 
            <td>@item.Tarih</td>
            <td><a href="/Deneyim/DeneyimSil/@item.Id" class="btn btn-danger ">Sil</a></td>
            <td><a href="/Deneyim/DeneyimGetir/@item.Id" class="btn btn-warning">Düzenle</a></td>
        </tr>
    }
    <tr></tr>
</table>
<a href="Deneyim/DeneyimEkle" class="btn btn-primary">Yeni Deneyim Ekle</a>
  