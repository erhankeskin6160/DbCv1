@model  DbCv1.Models.Entity.TblDeneyimler
@{
    ViewBag.Title = "DeneyimGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Deneyim Düzenleme Paneli</h2>

 
<br />
<form method="post">
    @Html.Label("Deneyim Id");
    @Html.TextBoxFor(x => x.Id, new { @class = "form-control" })

    @Html.Label("Baslık")
    @Html.TextBoxFor(x => x.Baslık, new { @class = "form-control" })

    @Html.Label("Alt Baslık")
    @Html.TextBoxFor(x => x.Alt_Baslık, new { @class = "form-control" })



    @Html.Label("Tarih")
    @Html.TextBoxFor(x => x.Tarih, new { @class = "form-control" })


    @Html.Label("Acıklama")
    @Html.TextAreaFor(x=>x.Aciklama,new {@class="form-control"})

    <br />


    <button class="btn btn-info">Güncelle</button>
</form>