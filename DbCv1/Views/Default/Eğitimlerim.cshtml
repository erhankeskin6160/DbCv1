@using DbCv1.Models.Entity

@model  List<TblEğitimlerim>




<div class="resume-section-content">
    <h2 class="mb-5">Education</h2>
    @foreach (var item in Model)
    {


    <div class="d-flex flex-column flex-md-row justify-content-between mb-5">
        <div class="flex-grow-1">
            <h3 class="mb-0">@item.Baslık</h3>
            <div class="subheading mb-3">@item.AltBaslık</div>
            <div>@item.AltBaslık2</div>
            <p>@item.GNO</p>
        </div>
        <div class="flex-shrink-0 "><span class="text-primary">@item.Tarih</span></div>
    </div>
    
    }
</div>
    

